{
  "name": "micro-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for Micro",
  "engines": {
    "npm": ">=5"
  },
  "scripts": {
    "start": "npm-run-all --parallel dev lint:watch",
    "dev": "webpack-dev-server",
    "prod": "cross-env NODE_ENV=production webpack-dev-server -p --env.prod=true",
    "build": "npm run clean-build && cross-env NODE_ENV=production webpack -p --env.prod=true --env.sw=true",
    "lint": "esw webpack.config.js src --color --format node_modules/eslint-friendly-formatter",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix"
  },
